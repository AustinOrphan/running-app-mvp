{
  "threshold": 10,
  "reporters": ["html", "json", "console"],
  "ignore": [
    "**/node_modules/**",
    "**/dist/**",
    "**/coverage/**",
    "**/*.test.ts",
    "**/*.test.tsx",
    "**/*.spec.ts",
    "**/*.spec.tsx",
    "**/tests/**"
  ],
  "gitignore": true,
  "output": "./reports/jscpd",
  "format": [
    "typescript",
    "javascript",
    "jsx"
  ],
  "min-lines": 5,
  "min-tokens": 50,
  "maxLines": 1000,
  "maxSize": "100kb",
  "formatsExts": {
    "typescript": ["ts", "tsx"],
    "javascript": ["js", "jsx"]
  },
  "blame": true,
  "silent": false,
  "absolute": false,
  "debug": false
}